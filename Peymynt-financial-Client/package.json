{
    "name": "peymynt",
    "version": "1.1.0",
    "private": true,
    "scripts": {
        "clean": "rimraf dist/*",
        "build:dev": "npm run clean && webpack --progress --bail --env dist",
        "build:pre": "npm run clean && webpack --progress --bail --env pre",
        "build:prod": "npm run clean && webpack --progress --bail --env prod -p",
        "lint": "eslint ./src",
        "lint-fix": "eslint ./src --fix",
        "lint-sass": "sass-lint -c .sass-lint.yml 'src/styles/**/*.scss' -v -q",
        "posttest": "npm run lint",
        "serve:dev": "webpack-dev-server --open --env dev",
        "serve:local": "webpack-dev-server --open --env local",
        "serve:dist": "node server.js",
        "serve:heroku": "node server.js",
        "serve": "node server.js",
        "start": "npm run serve:dev",
        "local": "npm run serve:local",
        "deploy:dev": "npm run build:dev && firebase deploy --only hosting:dev",
        "deploy:pre": "npm run build:pre && firebase deploy --only hosting:pre",
        "deploy:beta": "npm run build:beta && firebase deploy --only hosting:beta"
    },
